{
  "name": "livret-accueil",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "generate:importmap": "payload generate:importmap"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/lib-storage": "^3.637.0",
    "@payloadcms/db-mongodb": "3.0.0-beta.90",
    "@payloadcms/db-postgres": "3.0.0-beta.90",
    "@payloadcms/live-preview-react": "3.0.0-beta.90",
    "@payloadcms/next": "3.0.0-beta.90",
    "@payloadcms/plugin-cloud-storage": "3.0.0-beta.90",
    "@payloadcms/richtext-lexical": "3.0.0-beta.90",
    "@payloadcms/richtext-slate": "3.0.0-beta.90",
    "@payloadcms/storage-vercel-blob": "3.0.0-beta.90",
    "@payloadcms/ui": "3.0.0-beta.90",
    "@webiny/react-rich-text-renderer": "5.39.11",
    "babel-plugin-react-compiler": "^0.0.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "next": "15.0.0-canary.104",
    "payload": "3.0.0-beta.90",
    "react": "19.0.0-rc-06d0b89e-20240801",
    "react-dom": "19.0.0-rc-06d0b89e-20240801",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@payloadcms/graphql": "3.0.0-beta.90",
    "@types/node": "^20.16.1",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-canary.104",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "typescript": "5.5.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.0",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  }
}